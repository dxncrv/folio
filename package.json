{
	"name": "folio",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"pb:pull": "node pb/scripts/sync.js pull",
		"pb:push": "node pb/scripts/sync.js push",
		"pb:typegen": "node pb/scripts/typegen.js",
		"pb:sync": "node pb/scripts/sync.js pull && node pb/scripts/typegen.js --from-file"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/enhanced-img": "^0.9.3",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.2.2",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^16.5.0",
		"iconify-icon": "^3.0.2",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.50.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1"
	},
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=10.0.0"
	},
	"packageManager": "pnpm@10.28.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"ignoredBuiltDependencies": [
			"sharp"
		]
	},
	"dependencies": {
		"@fontsource/averia-libre": "^5.2.7",
		"@fontsource/averia-sans-libre": "^5.2.7",
		"@fontsource/averia-serif-libre": "^5.2.7",
		"@fontsource/fira-code": "^5.2.7",
		"@fontsource/hanken-grotesk": "^5.2.8",
		"@fontsource/redacted-script": "^5.2.8",
		"@fontsource/solway": "^5.2.8",
		"pocketbase": "^0.26.8"
	}
}
