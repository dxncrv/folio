		"@fontsource/averia-serif-libre": "^5.2.6",
		"@fontsource/fira-code": "^5.2.6",
		"@fontsource/hanken-grotesk": "^5.2.7",
		"@fontsource/solway": "^5.2.6",
		"ioredis": "^5.7.0"
	}
}
